<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
{{#if currentUser}}
    <b>User:</b>
    <span>{{currentUser.email}}</span>
{{/if}}
{{#if users}}
    <h1>All users</h1>
    {{#each users}}
        <div>{{this.email}}</div>
    {{/each}}
{{/if}}
</body>
</html>